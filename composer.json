{
    "name": "tsufeki/php-lenient-parser",
    "description": "A PHP parser written in PHP, handles partial/illegal input",
    "keywords": ["php", "parser"],
    "type": "library",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "tsufeki"
        }
    ],
    "require": {
        "php": ">=7.0",
        "ext-tokenizer": "*",
        "nikic/php-parser": "^4.0 || dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "PhpLenientParser\\": "lib/PhpLenientParser"
        }
    },
    "bin": ["bin/php-lenient-parse"]
}
