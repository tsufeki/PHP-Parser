{
    "name": "tsufeki/php-lenient-parser",
    "description": "A PHP parser written in PHP, handles partial/illegal input",
    "keywords": ["php", "parser"],
    "type": "library",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "tsufeki"
        },
        {
            "name": "Nikita Popov",
            "role": "Original author"
        }
    ],
    "require": {
        "php": ">=5.6",
        "ext-tokenizer": "*",
        "nikic/php-parser": "dev-identifiersAsNodes"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0|~5.0"
    },
    "autoload": {
        "psr-4": {
            "PhpLenientParser\\": "lib/PhpLenientParser"
        }
    },
    "bin": ["bin/php-lenient-parse"]
}
